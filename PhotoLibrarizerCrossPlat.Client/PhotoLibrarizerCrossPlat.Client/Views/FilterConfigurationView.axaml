<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:PhotoLibrarizerCrossPlat.Client.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="PhotoLibrarizerCrossPlat.Client.Views.FilterConfigurationView"
             DataContext="{Binding RelativeSource={RelativeSource Self}}"
             x:DataType="viewModels:FilterConfigurationViewModel"
             x:CompileBindings="False">
    <StackPanel Margin="20">
        <TextBlock Text="Filter Configuration" FontSize="18" FontWeight="Bold"/>
        
        <!-- Extensions -->
        <ComboBox Name="ExtensionsComboBox" PlaceholderText="Extensions" 
                  
                  />
      
    </StackPanel>
</UserControl>
